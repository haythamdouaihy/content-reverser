# Editor configuration, see http://editorconfig.orgroot = true[*]charset = utf-8indent_style = tabsindent_size = 2tab_width = 4end_of_line = crtrim_trailing_whitespace = trueinsert_final_newline = true[*.js]max_line_length = 120[*.md]max_line_length = offtrim_trailing_whitespace = false